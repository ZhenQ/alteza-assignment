{
  "name": "zhen/alteza-assignment",
  "description": "Alteza assignment by Zhen",
  "license": "proprietary",
  "type": "project",
  "require": {
    "php": "~7.1",
    "ext-json": "*",
    "ext-pdo": "*",
    "twig/twig": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "helpers/strings.php"
    ]
  },
  "scripts": {
    "serve": [
      "php -S localhost:8080 public/index.php"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
